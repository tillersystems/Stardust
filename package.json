{
  "name": "@tillersystems/stardust",
  "version": "4.2.1",
  "description": "Tiller Design System",
  "author": {
    "name": "Tillersystems",
    "url": "https://www.tillersystems.com/"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "clean": "rimraf lib dist es",
    "test": "BABEL_ENV=test jest",
    "test:cov": "yarn test --coverage",
    "test:ci": "BABEL_ENV=test jest --ci --maxWorkers=2",
    "lint": "eslint src/**/*.js",
    "format": "prettier --config .prettierrc --color --write \"src/**/*.js\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "yarn clean && yarn test:ci && yarn build:commonjs && yarn build:es",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublishOnly": "yarn build",
    "release": "./internal/tools/release.sh",
    "storybook-build": "build-storybook -c .storybook -o .out"
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "eslint": "./node_modules/.bin/eslint",
    "jest": "./node_modules/.bin/jest",
    "prettier": "./node_modules/.bin/prettier",
    "conventional-changelog": "./node_modules/.bin/conventional-changelog"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && yarn lint",
      "pre-push": "yarn run test"
    }
  },
  "peerDependencies": {
    "prop-types": "^15.x",
    "react": "^16.x",
    "react-dom": "^16.x",
    "react-pose": "^4.x",
    "styled-components": "^4.x"
  },
  "dependencies": {
    "@tippy.js/react": "2.2.2",
    "initials": "3.0.1",
    "luxon": "1.11.4",
    "polished": "3.4.1",
    "react-onclickoutside": "6.8.0",
    "react-portal": "4.2.0",
    "styled-components-breakpoint": "2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.5.0",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.2",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.4",
    "@nivo/bar": "0.55.0",
    "@sambego/storybook-state": "1.3.6",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-info": "5.1.9",
    "@storybook/addon-knobs": "5.1.9",
    "@storybook/addon-links": "5.1.9",
    "@storybook/addon-options": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/react": "5.1.9",
    "@storybook/theming": "5.1.9",
    "@testing-library/react": "8.0.4",
    "@tillersystems/eslint-config": "1.1.8",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.6",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "conventional-changelog-cli": "2.0.21",
    "core-js": "3.1.4",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "jest-dom": "3.5.0",
    "jest-styled-components": "6.3.3",
    "json": "9.0.6",
    "mockdate": "2.0.3",
    "popmotion": "8.7.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-pose": "4.0.8",
    "storybook-readme": "5.0.5",
    "styled-components": "4.3.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/internal/",
      ".*.test\\.js$",
      ".*.stories\\.js$"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "internal/jest",
      "__dirname"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internal/tools/mocks/fileMock.js"
    },
    "setupFilesAfterEnv": [
      "./internal/jest/index.js"
    ],
    "testRegex": ".*.test\\.js$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
